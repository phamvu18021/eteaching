<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_th_major_tree" model="ir.ui.view">
        <field name="name">th.major.tree</field>
        <field name="model">th.major</field>
        <field name="arch" type="xml">
            <tree string="Views" editable="top">
                <field name="name"/>
                <field name="description"/>
                <field name="th_code"/>

            </tree>
        </field>
    </record>
    <record id="th_major_model_action" model="ir.actions.act_window">
        <field name="name">Chuyên ngành</field>
        <field name="res_model">th.major</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
