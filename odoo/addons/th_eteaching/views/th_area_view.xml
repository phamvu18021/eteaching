<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_th_area_tree" model="ir.ui.view">
        <field name="name">th.area.tree</field>
        <field name="model">th.area</field>
        <field name="arch" type="xml">
            <tree string="Views" editable="top">
                <field name="name"/>
                <field name="description"/>
                <field name="th_code"/>
            </tree>
        </field>
    </record>

    <record id="th_area_model_action" model="ir.actions.act_window">
        <field name="name">Phạm vi tuyển sinh</field>
        <field name="res_model">th.area</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>

