<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="th_training_channel_tree_view" model="ir.ui.view">
        <field name="name">th.training.channel.tree.view</field>
        <field name="model">th.training.channel</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name"/>
                <field name="description"/>
                <field name="th_code"/>
            </tree>
        </field>
    </record>

    <record id="th_training_channel_action" model="ir.actions.act_window">
        <field name="name">Hình thức đào tạo</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">th.training.channel</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>