<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_th_station_tree" model="ir.ui.view">
        <field name="name">th.station.tree</field>
        <field name="model">th.station</field>
        <field name="arch" type="xml">
            <tree string="Views" editable="top">
                <field name="name"/>
                <field name="th_code"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <record id="th_station_model_action" model="ir.actions.act_window">
        <field name="name">Trạm tuyển sinh</field>
        <field name="res_model">th.station</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>