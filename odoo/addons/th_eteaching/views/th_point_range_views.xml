<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_th_point_range_tree" model="ir.ui.view">
        <field name="name">th.point.range.tree</field>
        <field name="model">th.point.range</field>
        <field name="arch" type="xml">
            <tree string="Views" editable="top">
                <field name="name"/>
                <field name="start_point"/>
                <field name="end_point"/>
                <field name="th_code"/>
            </tree>
        </field>
    </record>

    <record id="th_point_range_model_action" model="ir.actions.act_window">
        <field name="name">Khoảng Điểm</field>
        <field name="res_model">th.point.range</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
